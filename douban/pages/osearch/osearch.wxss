.section{
    padding:10px;     
}
.header{
    padding:15px 18px;
    border-bottom: 1px solid #eee;
}
.header navigator,.header form{
    display: inline-block;
}
.header navigator{
    color:#42bd56;
    font-size: 16px;
    margin-right:10px; 
}
.header form{
    width:87%;
}
.header input{
    width: 90%;
    background: #f2f2f2;
    font-size: 16px;
    padding: 8px 16px;
    border-radius: 5px;
}
.kind-list{
    display: inline-block;
    width: 33.3333%;
    text-align: center;
    padding: 15px 0;
}
.title{
    font-size: 22px;
    font-weight: bold;
}
.cont{
    font-size: 12px;
    color:#999;
    margin-top: 3px;
}